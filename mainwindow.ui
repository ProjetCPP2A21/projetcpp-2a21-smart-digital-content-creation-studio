        <property name="styleSheet">
         <string notr="true">QGroupBox {
    border: 2px solid #A64B00;
    border-radius: 8px;
    margin-top: 20px;
    background: qlineargradient(
        x1:0, y1:0, x2:1, y2:1,
        stop:0 white,
        stop:1 #FB8C00
    );
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 4px;
    color: white;
    font-weight: bold;
}

QGroupBox * {
    background: transparent;
    color: black;
}

QGroupBox QPushButton {
    background: #6A0DAD;         /* Mauve */
    color: white;
    border: 1px solid #4B0082;
    border-radius: 6px;
    padding: 5px 10px;
}

QGroupBox QPushButton:hover {
    background: #1B1B3A;         /* Bleu foncé au survol */
}

QGroupBox QComboBox {
    background: #6A0DAD;         /* Mauve */
    color: white;
    border: 1px solid #4B0082;
    border-radius: 6px;
    padding: 4px 8px;
}

QGroupBox QComboBox:hover {
    background: #1B1B3A;         /* Bleu foncé au survol */
}

QGroupBox QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #4B0082;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    background: #6A0DAD;         /* Même mauve que le QComboBox */
}

QGroupBox QComboBox::down-arrow {
    width: 12px;
    height: 12px;
}</string>
